{
  "name": "ui5-splash-screen-poc",
  "version": "1.0.0",
  "description": "UI5 Splash Screen POC with multiple environment configurations",
  "scripts": {
    "start": "npm run start:cdn",
    "start:cdn": "http-server -p 8300 --cors -o /index-configurable.html?env=cdn",
    "start:local": "npx ui5 serve --port 8300 --open index-configurable.html?env=local",
    "start:backend": "http-server -p 8300 --cors -o /index-configurable.html?env=backend",
    "start:hybrid": "npx ui5 serve --port 8300 --config ui5-backend.yaml --open index-configurable.html?env=hybrid",
    "build": "node build.js"
  },
  "keywords": [
    "ui5",
    "splash-screen",
    "sapui5",
    "sap"
  ],
  "author": "ac4y",
  "license": "MIT",
  "devDependencies": {
    "@ui5/cli": "^4.0.43",
    "cross-env": "^7.0.3",
    "http-server": "^14.1.1",
    "ui5-middleware-simpleproxy": "^3.6.2"
  }
}
