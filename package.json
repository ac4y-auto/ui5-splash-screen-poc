{
  "name": "ui5-splash-screen-poc",
  "version": "1.0.0",
  "description": "UI5 Splash Screen POC with fiori-tools-proxy",
  "scripts": {
    "start": "npx fiori run --port 8300 --open index.html",
    "start:backend": "npx fiori run --port 8300 --config ui5-backend.yaml --open index.html",
    "smart-start": "node start.js",
    "smart-start:backend": "node start.js ui5-backend.yaml"
  },
  "keywords": [
    "ui5",
    "splash-screen",
    "sapui5",
    "sap",
    "fiori"
  ],
  "author": "ac4y",
  "license": "MIT",
  "devDependencies": {
    "@sap/ux-ui5-tooling": "^1.20.2",
    "@ui5/cli": "^4.0.43"
  }
}
